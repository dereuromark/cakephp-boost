{
    "name": "dereuromark/cakephp-boost",
    "description": "CakePHP Boost - Development tool for AI-assisted coding with CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "plugin", "boost", "ai", "documentation", "search"],
    "homepage": "https://github.com/dereuromark/cakephp-boost",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Scherer",
            "homepage": "https://www.dereuromark.de",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "cakephp/cakephp": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "CakeBoost\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeBoost\\Test\\": "tests/"
        }
    }
}
